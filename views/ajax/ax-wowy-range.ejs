                <th>FF%</th>
                <th>FF/60</th>
                <th>FA/60</th>
              </tr>
            </thead>
            <tbody>
              <% for (var i = 0; i < data.length; i++) { %>
                <% if(data[i].type === '1 and 2') { %>
